{
  "name": "formualizer-wasm",
  "version": "0.1.0",
  "description": "WebAssembly bindings for Formualizer Excel formula parser",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "pkg"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "wasm-pack build --target web --out-dir pkg --release",
    "build:js": "rollup -c",
    "test": "wasm-pack test --node",
    "clean": "rimraf dist pkg"
  },
  "keywords": [
    "excel",
    "formula",
    "parser",
    "wasm",
    "webassembly",
    "spreadsheet"
  ],
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/formualizer"
  }
}